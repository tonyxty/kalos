def read_int() -> int extern;
def println(n: int, ...) extern;

def hanoi(n: int, x: int, y: int, z: int) {
	if (n) {
		hanoi(n - 1, x, z, y);
		println(2, x, z);
		hanoi(n - 1, y, x, z);
	} else {
		println(2, x, z);
	}
	return 0;
}

def main() {
	var n = read_int();
	hanoi(n - 1, 1, 2, 3);
	return 0;
}
